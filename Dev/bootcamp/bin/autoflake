#!/media/netunit/storage/SoftServeItAcademy/Dev/bootcamp/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'autoflake==0.6.6','console_scripts','autoflake'
__requires__ = 'autoflake==0.6.6'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('autoflake==0.6.6', 'console_scripts', 'autoflake')()
    )
