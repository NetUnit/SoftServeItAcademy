#!/media/netunit/storage/SoftServeItAcademy/Dev/bootcamp/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'vulture==0.10','console_scripts','vulture'
__requires__ = 'vulture==0.10'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vulture==0.10', 'console_scripts', 'vulture')()
    )
