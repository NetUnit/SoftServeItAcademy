#!/media/netunit/storage/SoftServeItAcademy/Dev/bootcamp/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'rstcheck==2.2','console_scripts','rstcheck'
__requires__ = 'rstcheck==2.2'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('rstcheck==2.2', 'console_scripts', 'rstcheck')()
    )
